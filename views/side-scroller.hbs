<table class="t_center align_top display_inline_block">
  <tr>
    <td colspan="3" class="align_top">
      <select id="data_packs">
        {{#each data_packs}}
          <option value='{{name}}' {{selected}}>{{name}}</option>
        {{/each}}
      </select>
    </td>
  </tr>
  <tr>
    <td colspan="3" id="side_scroller" class="align_top">
    </td>
  </tr>
  <tr>
    <td class="align_top">
      <button id="arrow-left" onclick="return moveLeft()">
        <img src="feathericons/arrow-left.svg" />
      </button>
      <button id="thumbs-up" onclick="return moveUp()">
        <img src="feathericons/arrow-up.svg" />
      </button>
      <button id="thumbs-down" onclick="return moveRight()">
        <img src="feathericons/arrow-right.svg" />
      </button>
    </td>
  </tr>
  <tr>
    <td class="align_top">
      <span id="score" class="small">Score: loading...</span>
      <span id="session" class="small">Session: loading...</span><br>
      <input id="account" type="text" size="66" max_length="64" onchange="updateAccount(); return false;" oninput="updateAccount(); return false;"></input>
    </td>
  </tr>
</table>
<script src="js/bm-captcha.js"></script>
<script src="js/views/side-scroller.js"></script>
