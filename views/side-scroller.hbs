<table class="t_center align_top display_inline_block">
  <tr>
    <td colspan="3" id="side_scroller" class="align_top">
    </td>
  </tr>
  <tr>
    <td class="align_top">
      <button id="arrow-left" onclick="return moveLeft()">
        <img src="feathericons/arrow-left.svg" />
      </button>
      <button id="thumbs-up" onclick="return moveUp()">
        <img src="feathericons/arrow-up.svg" />
      </button>
      <button id="thumbs-down" onclick="return moveRight()">
        <img src="feathericons/arrow-right.svg" />
      </button>
    </td>
  </tr>
  <tr>
    <td class="align_top">
      <span id="score">Score: loading...</span><br>
      <input id="account" type="text" size="66" max_length="64" onchange="updatAccount(); return false;" oninput="updatAccount(); return false;"></input>
    </td>
  </tr>
</table>
<script src="js/bm-captcha.js"></script>
<script src="js/views/side-scroller.js"></script>
